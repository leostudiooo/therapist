{
  "type": "extension",
  "name": "gemini_llm_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    }
  ],
  "api": {
    "property": {
      "properties": {
        "api_key": {
          "type": "string"
        },
        "greeting": {
          "type": "string"
        },
        "max_memory_length": {
          "type": "int64"
        },
        "max_output_tokens": {
          "type": "int64"
        },
        "model": {
          "type": "string"
        },
        "prompt": {
          "type": "string"
        },
        "temperature": {
          "type": "float64"
        },
        "top_k": {
          "type": "int64"
        },
        "top_p": {
          "type": "float64"
        }
      }
    },
    "cmd_in": [
      {
        "name": "flush"
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      }
    ],
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "properties": {
            "text": {
              "type": "string"
            }
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "properties": {
            "text": {
              "type": "string"
            }
          }
        }
      }
    ]
  }
}