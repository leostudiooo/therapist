{
  "type": "extension",
  "name": "emotion_bridge_python",
  "version": "0.1.0",
  "language": "python",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10.25"
    }
  ],
  "api": {
    "property": {
      "websocket_url": {
        "type": "string",
        "default": "ws://localhost:8765"
      },
      "reconnect_interval": {
        "type": "float64",
        "default": 5.0
      },
      "emotion_threshold": {
        "type": "float64",
        "default": 0.5
      },
      "enable_stale_filter": {
        "type": "bool",
        "default": true
      },
      "max_reconnect_attempts": {
        "type": "int64",
        "default": 10
      }
    },
    "data_out": [
      {
        "name": "emotion_context",
        "property": {
          "emotional_state": "string",
          "attention": "float64",
          "engagement": "float64",
          "excitement": "float64",
          "stress": "float64",
          "relaxation": "float64",
          "timestamp": "string"
        }
      }
    ]
  }
}